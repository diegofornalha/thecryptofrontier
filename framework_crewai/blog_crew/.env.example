# Configurações de API - Copie este arquivo para .env e preencha com suas credenciais

# OpenAI API para geração de imagens com DALL-E 3
OPENAI_API_KEY=sua_chave_openai_aqui

# Google API para os agentes CrewAI
GOOGLE_API_KEY=sua_chave_google_aqui

# Sanity CMS
SANITY_PROJECT_ID=seu_projeto_id_aqui
SANITY_API_TOKEN=seu_token_com_permissao_escrita_aqui
SANITY_DATASET=production

# Algolia Search
ALGOLIA_APP_ID=seu_app_id_aqui
ALGOLIA_SEARCH_API_KEY=sua_search_key_aqui
ALGOLIA_ADMIN_API_KEY=sua_admin_key_aqui
ALGOLIA_INDEX_NAME=seu_index_aqui
ALGOLIA_WRITE_API_KEY=sua_write_key_aqui